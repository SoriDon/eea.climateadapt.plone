              <tal:macro metal:define-macro="assessment-climate-hazards">
                <ul tal:condition="values">
                  <li tal:repeat="val python: values.split(', ')"
                      tal:content="python: val.split(' - ')[-1]">Value here</li>
                  <li tal:condition="other_values">
                    Other: <span tal:content="other_values"></span>
