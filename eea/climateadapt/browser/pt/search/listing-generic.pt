<table class="listing-table" tal:define="brains view/brains; nr_res python: len(brains);">
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
    </tr>
  </thead>
  <tbody tal:define="
    two python: nr_res / 5;
    three python: 2 * two;
    four python: 3 * two;
    five python: 4 * two;">
    <tr tal:repeat="brain brains">
      <td>
        » <a href="" tal:content="brain/Title" tal:attributes="href brain/getURL">» Link</a>
      </td>
      <td class="table_year_css" tal:content="python: brain.year or '' ">year</td>
    </tr>
  </tbody>
</table>
