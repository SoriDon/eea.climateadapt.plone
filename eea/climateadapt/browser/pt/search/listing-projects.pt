<table class="listing-table" tal:define="brains view/brains; nr_res python: len(brains);">
  <thead>
    <tr>
      <th>Title</th>
    </tr>
  </thead>
  <tbody tal:define="
    two python: nr_res / 5;
    three python: 2 * two;
    four python: 3 * two;
    five python: 4 * two;">
    <tr tal:repeat="brain brains">
      <td tal:define="title brain/Title">
        »<span tal:condition="brain/acronym">
            <a href="" tal:content="brain/acronym" tal:attributes="href brain/getURL">» Link</a>
        </span>
        - <span tal:replace="python: title">
        </span> ...
      </td>
    </tr>
  </tbody>
</table>
