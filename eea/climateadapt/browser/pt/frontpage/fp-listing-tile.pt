<div tal:attributes="class view/__name__">
  <h2><a href="" tal:content="view/title" tal:define="url view/parent/absolute_url" tal:attributes="href url"></a></h2>
  <ul tal:define="items view/items">
    <li tal:repeat="obj items">
      <a href="" tal:attributes="href python: view.get_external_url(obj)">
          <span tal:content="obj/Title"></span>
          <p tal:content="python: view.get_item_date(obj)" class="news-time"></p>
      </a>
    </li>
  </ul>
  <div class="tile-listing-footer">
    <a href="" tal:define="info view/more_url; title python: info[1]; url python: info[0]"
      tal:attributes="href url" tal:content="title"></a>
  </div>
</div>
