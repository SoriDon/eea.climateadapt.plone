<div class="row">
  <div class="column col-md-16 " data-panel="">
    <div class="tile tile-default" id="b48b97e5-278f-4474-a61b-b73734177559">
      <div class="cover-richtext-tile tile-content">
        <div class="cover-richtext-tile tile-content"><div style="float:right;"><a class="right standard-button secundary-button" href="/knowledge/european-climate-data-explorer">Go back</a></div>
          <h1>Overview list all indices</h1>
          <p tal:content="structure view/data/overview_page/description"></p>
        </div>
      </div>
    </div>
  </div>

  <div class="row" tal:define="categories view/get_categories">
    <div class="column col-md-6" tal:repeat="category categories">
        <h2 tal:content="category/name">Heat and cold</h2>
        <tal:def define="subhazards category/data">
            <tal:rep repeat="subhazard subhazards">
            <h3 tal:content="subhazard" >Extreme heat</h3>
            <ul tal:define="indicators subhazards/?subhazard">
              <li tal:repeat="info indicators">
                <a href=""
                  tal:attributes="href python:view.json_indicator_page_to_url(info['url'])"
                  tal:content="info/title">Maximum daily</a>
              </li>
            </ul>
            </tal:rep>
        </tal:def>
    </div>
  </div>

