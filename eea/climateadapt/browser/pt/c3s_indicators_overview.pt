      <tal:def define="subhazards data/hazard_list/?hazard_title">
        <tal:rep repeat="subhazard subhazards">
          <h3 tal:condition="python: len(data['hazard_list'][hazard_title]) > 1" tal:content="subhazard" >Extreme heat</h3>
          <ul tal:define="indicators data/hazard_list/?hazard_title/?subhazard">
            <li tal:repeat="info indicators">
              <a href=""
                tal:attributes="href python:view.json_indicator_page_to_url(info['url'])"
                tal:content="info/title">Maximum daily</a>
            </li>
          </ul>

        </tal:rep>
      </tal:def>
    </div>
  </div>


