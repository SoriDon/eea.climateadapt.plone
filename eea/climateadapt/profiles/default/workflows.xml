<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool">
    <property
        name="title">Contains workflow definitions for your portal</property>
    <object name="cityprofile_workflow" meta_type="Workflow"/>
    <object name="cca_items_workflow" meta_type="Workflow"/>
    <object name="newsevents_workflow" meta_type="Workflow"/>
    <bindings>
        <type type_id="Event">
            <bound-workflow workflow_id="newsevents_workflow"/>
        </type>
        <type type_id="News Item">
            <bound-workflow workflow_id="newsevents_workflow"/>
        </type>
        <default>
            <bound-workflow workflow_id="simple_publication_workflow"/>
        </default>
        <type type_id="eea.climateadapt.city_profile">
            <bound-workflow workflow_id="cityprofile_workflow"/>
        </type>
        <type type_id="eea.climateadapt.acemeasure">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.aceproject">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.adaptationoption">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.casestudy">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.guidancedocument">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.indicator">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.informationportal">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.mapgraphdataset">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.organisation">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.publicationreport">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.researchproject">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
        <type type_id="eea.climateadapt.tool">
            <bound-workflow workflow_id="cca_items_workflow"/>
        </type>
    </bindings>
</object>
