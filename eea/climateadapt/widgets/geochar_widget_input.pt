    <script>
      jQuery(document).ready(function(){
        $(".europe_geochar_class").hide();

        $("#europe_geo_chars").change(function(){
          $(".europe_geochar_class").toggle();
        });

        $("#global_geo_chars").change(function(){
          $(".europe_geochar_class").toggle();
        });

        function updateForm(){
          var text = $("#form-widgets-geochars").val();
          if (text.length === 0) return

          var config = JSON.parse(text);
          geo = config.geoElements;

          $("input[name='rad_geo_chars']").each(function(i, el){
            if ($(el).val() === geo.element) $(el).attr('checked', 'checked');
          });

          if (geo.element === 'EUROPE') $('.europe_geochar_class').show();

          $("#trans_macro_nationals").val(geo.macrotrans);
          $("#trans_bio_nationals").val(geo.biotrans);

          var countries = [];
          if (geo.countries !== null) {
            countries = geo.countries;
          }

          $(".chk_countries_geochars").each(function(i, el){
            if (countries.indexOf($(el).val()) > -1) {
              $(el).attr('checked', 'checked');
            }
          });

          // Checks on page load if there are any inputs checked and
          // shows the options for the countries that are checked
          var subs = $('#subnationals').children();
          $(subs).each(function (i, el) {
            for (index = 0; index < this.length; index++) {
              if (el.text.indexOf(this[index]) > -1) {
                $(el).show();
                break;
              }
              else {
                $(el).hide();
              }
            }
          }.bind(countries));

          // If no inputs are checked, don't show any options on page load
          if (countries.length === 0) {
            $(subs).each(function(e, el){
              $(el).hide()
            });
          }

          $(".shared_form_city").val(geo.city);
        }
        updateForm();

        function updateJson(){
          var obj = {};
          var geo = {};
          geo.element = $("input[name='rad_geo_chars']:checked").val().toString();
          if (geo.element === 'GLOBAL') {
            geo.macrotrans = [];
            geo.biotrans = [];
            geo.countries = [];
            geo.subnational = [];
            geo.city = '';
          } else {
            geo.macrotrans = $("#trans_macro_nationals").val();
            geo.biotrans = $("#trans_bio_nationals").val();
            geo.countries = $(".chk_countries_geochars:checked").map(function(i, el){return el.value;}).toArray();
            geo.subnational = $("#subnationals_selected option").map(function(i, el){return el.value;}).toArray();
            geo.city = $(".shared_form_city").val();
          }
          obj.geoElements = geo;
          $("#form-widgets-geochars").val(JSON.stringify(obj));
        }

        $("#formfield-form-widgets-geochars :input").change(updateJson);

        $("#to2").click(function(){
          $("#subnationals :selected").clone().appendTo($("#subnationals_selected"));
          updateJson();
        });

        $("#to1").click(function(){
          $("#subnationals_selected :selected").remove();
          updateJson();
        });
      });
    </script>
