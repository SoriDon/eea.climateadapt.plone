<!DOCTYPE html>
<html lang="en">
  <head>
    {{> partials/head }}
  </head>

  <body>
    {{> partials/edit-menu }}
    {{> partials/health-globalmenu }}
    {{> partials/health-top }}

    <div class="content-container site-container main-area">
      <div class="row" id="content" class="health-homepage-specific">
        <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12 health-left">
          {{> partials/health-sidebar }}
          <div class="health_contribute">
            <h4>Contribute to the Observatory</h4>
