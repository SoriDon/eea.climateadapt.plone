  <script type="text/javascript">
    $(document).ready(function() {
      var c = document.location.href;
      $("#uast-menu").accordion({
        autoHeight: false,
        navigation: true,
        navigationFilter: function() {
          var e = c.substr(document.location.href.indexOf("ast/step") + 4);
          var d = $(this).attr("href");
          d = d.substr(d.indexOf("ast/step") + 4);
          if (e.indexOf("?") > 0) {
            e = e.substr(0, e.indexOf("?"))
          }
          if ((e.indexOf(d) != -1)) {
            $(this).addClass("active");
            return true;
          } else {
            return false;
          }
        }
      });

      var activeNav = $("#uast-menu a.active");
      var anchors = $("#uast-menu a");

      var pos = anchors.index(activeNav);
      var next = anchors.get(pos+1);
      var previous = anchors.get(pos-1);

      $("#previous-link a").attr("href", previous);
      $("#next-link a").attr("href", next);
    });
  </script>

