    <script type="text/javascript">
        $(document).ready(function() {
            var c = document.location.href;
            $("#ast-menu").accordion({
                autoHeight: false,
                navigation: true,
                navigationFilter: function() {
                    var e = c.substr(document.location.href.indexOf("ast/step") + 4);
                    var d = $(this).attr("href");
                    d = d.substr(d.indexOf("ast/step") + 4);
                    if (e.indexOf("?") > 0) {
                        e = e.substr(0, e.indexOf("?"))
                    }
                    if ((e.indexOf(d) != -1)) {
                        $(this).addClass("active");
                        return true
                    } else {
                        return false
                    }
                }
            });
            $("a.ui-state-active").parent().css("background-color", "#6D7A2B");
            var b = new Array();
            $("#ast-menu li a").each(function() {
                b.push($(this).attr("href"))
            });
            var a = b.length;
            i = 0;
            pos = -1;
            previous = "";
            next = "";
            while (i < a && pos < 0) {
                pos = c.search(b[i]);
                if (pos > 0) {
                    if (i == 0) {
                        previous = b[a - 1];
                        next = b[1]
                    } else {
                        if (i == a - 1) {
                            previous = b[a - 2];
                            next = b[0]
                        } else {
                            previous = b[i - 1];
                            next = b[i + 1]
                        }
                    }
                }
                i++
            }
            $("#previous-link a").attr("href", previous);
            $("#next-link a").attr("href", next)
        });
    </script>

